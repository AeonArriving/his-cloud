<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwy.his.dao.PatientRegisterMapper">

      <insert id="addpatientuser" parameterType="com.lwy.his.entity.patient.PatientUser" >
         insert into patientuser (puusername,pupassword,puname,pusex,pubirthday,pucard,puaddress,
         puphone,pucreate,puhistory) values (#{puusername},#{pupassword},#{puname},#{pusex},#{pubirthday},
         #{pucard},#{puaddress},#{puphone},#{pucreate},#{puhistory});



      </insert>



</mapper>